<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="github" 
           fetch="https://github.com" />
  <remote  name="gist" 
           fetch="https://gist.github.com" />
  <remote  name="googlesource" 
           fetch="https://gerrit.googlesource.com" />

  <default revision="refs/heads/master"
           remote="github"
           sync-j="4" />

  <project remote="googlesource" name="git-repo" />
  <project remote="github" name="joeyh/myrepos" />
  <project remote="github" name="RichiH/vcsh" />
  <project remote="github" name="portante/pycscope"/>
  <project remote="github" name="bluss/git-remote-gcrypt"/>
  <project remote="github" name="aanand/git-up" />
  <project remote="gist" name="1978266" path="git-cache-meta" />
  <project remote="gist" name="5758221" path="git-prune" />
</manifest>

